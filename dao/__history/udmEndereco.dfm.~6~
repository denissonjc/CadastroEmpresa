  object qAlterarEndereco: TFDQuery
    Connection = dmConexao.SQLConexao
    SQL.Strings = (
      'update cadenderecos'
      'set idtitular = :idtitular,'
      '    idempresa = :idempresa,'
      '    nmendereco = :nmendereco,'
      '    nuendereco = :nuendereco,'
      '    nmbairro = :nmbairro,'
