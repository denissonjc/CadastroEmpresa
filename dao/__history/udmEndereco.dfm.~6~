  object qPesquisarEndereco: TFDQuery
    Connection = dmConexao.SQLConexao
    SQL.Strings = (
      'select *'
      'from cadenderecos c'
      'where c.nmendereco like :nome'
      'order by c.idendereco')
    Left = 48
    Top = 48
    ParamData = <
