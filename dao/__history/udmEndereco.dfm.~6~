  object qEnderecoGrid: TFDQuery
    Connection = dmConexao.SQLConexao
    SQL.Strings = (
      'select *'
      ' from cadenderecos cd'
      ' join cadempresa ce on cd.idempresa = ce.idempresa'
      ' where cd.idempresa = :idempresa')
    Left = 472
    Top = 56
